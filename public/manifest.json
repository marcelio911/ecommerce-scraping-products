{
  "short_name": "Ecommerce coleta produtos",
  "name": "Ecommerce coleta produtos",
  "manifest_version": 3,
  "devtools_page": "examples/devtools.html",
  "permissions": [
    "activeTab",
    "tabs",
    "debugger",
    "contextMenus",
    "webNavigation",
    "downloads",
    "storage",
    "power",
    "scripting"
  ],
  "host_permissions": [
    "https://shopee.com.br/*",
    "https://www.gstatic.com/firebasejs/*"
  ],
  "optional_host_permissions":[
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "sw.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Come√ßar a coletar produtos",
    "default_icon": {
      "16": "assets/images/favicon.png",
      "48": "assets/images/favicon.png",
      "128": "assets/images/favicon.png"
    }
  },
  "icons": {
    "16": "assets/images/favicon.png",
    "48": "assets/images/favicon.png",
    "128": "assets/images/favicon.png"
  },
  "version": "0.0.3"
}
